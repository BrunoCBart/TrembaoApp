{
  "name": "trembao-api",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server",
  "scripts": {
    "build": "/bin/sh tsc_eval.sh",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prestart": "npm run db:reset",
    "start": "node .",
    "predev": "npm run build && npm run db:reset",
    "dev": "ts-node-dev ./src/server",
    "pretest": "NODE_ENV=test npx sequelize-cli db:drop && NODE_ENV=test npx sequelize-cli db:create && NODE_ENV=test npx sequelize db:migrate && NODE_ENV=test npx sequelize-cli db:seed:all",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "NODE_ENV=test npx sequelize-cli db:drop",
    "test:coverage": "nyc npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-test-helpers": "^1.4.2",
    "sinon": "^13.0.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
